{
  "name": "@deskpro-apps/bookmarks",
  "title": "Bookmarks",
  "description": "Effortlessly save and organize your must have web pages with the Bookmarks App.",
  "version": "1.0.0",
  "scope": "agent",
  "isSingleInstall": false,
  "hasDevMode": true,
  "targets": [
    {
      "target": "global",
      "entrypoint": "index.html"
    }
  ],
  "settings": {
    "bookmarks": {
      "title": "",
      "type": "app_embedded",
      "options": {
        "entrypoint": "#/admin"
      },
      "isRequired": true,
      "isBackendOnly": false
    }
  },
  "proxy": {
    "whitelist": [
      {
        "url": "https://www.google.com/s2/favicons?domain=.*",
        "methods": [
          "GET"
        ],
        "timeout": 30
      }
    ]
  }
}
